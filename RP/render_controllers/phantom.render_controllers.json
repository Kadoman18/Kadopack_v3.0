{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.phantom": {
      "arrays": {
        "materials": {
          "Array.materials": [
            "Material.default",
            "Material.invisible"
          ]
        },
        "textures": {
          "Array.Xskins": [
            "Texture.default",
            "Texture.default2"
          ]
        }
      },
      "geometry": "Geometry.default",
      "materials": [
        {
          "*": "Array.materials[query.is_invisible]"
        }
      ],
      "textures": [
        "query.is_name_any('Phantom') ? Texture.default : query.is_name_any('Vulture') ? Texture.default2 : Array.Xskins[variable.Xvar]"
      ]
    }
  }
}