{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.husk": {
      "geometry": "Geometry.default",
      "materials": [
        {
          "*": "Material.default"
        }
      ],
      "textures": [
        "query.is_name_any('Husk') ? Texture.default : query.is_name_any('Chainmail') ? Texture.default2 : query.is_name_any('Armored') ? Texture.default3 : Array.Xskins[variable.Xvar]"
      ],
      "arrays": {
        "textures": {
          "Array.Xskins": [
            "Texture.default",
            "Texture.default2",
            "Texture.default3"
          ]
        }
      }
    }
  }
}
