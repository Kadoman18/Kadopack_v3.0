{
	"format_version": "1.8.0",
	"render_controllers": {
		"controller.render.spider": {
			"arrays": {
				"materials": {
					"Array.materials": ["Material.default", "Material.invisible"]
				},
				"textures": {
					"Array.Xskins": [
						"Texture.default",
						"Texture.blue",
						"Texture.green",
						"Texture.lime",
						"Texture.orange",
						"Texture.purple",
						"Texture.teal"
					]
				}
			},
			"geometry": "Geometry.default",
			"materials": [
				{
					"*": "Array.materials[q.is_invisible]"
				}
			],
			"textures": ["Array.Xskins[v.Xvar]"],
			"overlay_color": {
				"r": "(v.Avar>0 ? 1.0 : -1.0)",
				"g": "(v.Avar>0 ? 1.0 : -1.0)",
				"b": "(v.Avar>0 ? 1.0 : -1.0)",
				"a": "m.abs(v.Avar)/20*0.12"
			}
		}
	}
}
